<html lang="en">

    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Edit profile</title>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
            integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
        <link rel="stylesheet" href="public/css/register.css">

        <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>

    </head>

    <body>
        
        <%- include('../header',{loggedin:loggedIn}); -%>
        <form action="/editprofile" method="post">
            <div class="container mt-5">
                <div class="main-body">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="card sticky-top">
                                <div class="card-body">
                                    <div class="d-flex flex-column align-items-center text-center">
                                        <img src="<%= user.profile.image %>" alt="Admin" class="rounded-circle p-1 bg-primary" width="110">
                                        <div class="mt-3">
                                            <label>Enter your Full Name</label>
                                            <div class="mb-3">
                                                <input type="text" class="form-control" name="fullname" value="<%=user.profile.fullname%>" required>
                                            </div>
                                            
                                            <div class="mb-3">
                                                <h6>Enter your Headline</h6>
                                                <input type="text" class="form-control" name="headline" value="<%=user.profile.headline%>" required>
                                            </div>
                                            <!-- <p class="text-muted font-size-sm"><%=user.contactdetails.address.city%>,<%=user.contactdetails.address.state%>,<%=user.contactdetails.address.country%>.</p> -->
                                        </div>
                                        <div class="row m-2 ">
                                            <h6 class="col-sm-4 m-0 mt-2 mb-2 d-inline-block">Willing to help</h6>
                                            <div class="col-sm-8 mb-4 text-secondary d-inline-block">
                                                
                                                <% if(user.profile.willtohelp) { %>
                                                    <div class="form-check d-inline-block mt-3">
                                                        <input class="form-check-input" type="radio" name="willtohelp" value="true" id="flexRadioDefault1" checked>
                                                        <label class="form-check-label" for="flexRadioDefault1">Yes</label>
                                                    </div>
                                                    <div class="form-check d-inline-block">
                                                        <input class="form-check-input" type="radio" name="willtohelp" value="false" id="flexRadioDefault2" >
                                                        <label class="form-check-label" for="flexRadioDefault2">No</label>
                                                    </div>
                                                <%} else { %>
                                                    <div class="form-check d-inline">
                                                        <input class="form-check-input" type="radio" name="willtohelp" value="true" id="flexRadioDefault1">
                                                        <label class="form-check-label" for="flexRadioDefault1">Yes</label>
                                                    </div>
                                                    <div class="form-check d-inline">
                                                        <input class="form-check-input" type="radio" name="willtohelp" value="false" id="flexRadioDefault2"  checked>
                                                        <label class="form-check-label" for="flexRadioDefault2">No</label>
                                                    </div>
                                                <% } %>
                                            </div>
                                        </div>  
                                        <div class="mb-3">
                                            <h6>Enter year of joining in vvit</h6>
                                            <input type="Number" min="2007" max="2100" class="form-control" name="batch" value="<%=user.profile.batch%>" required>
                                        </div>
                                        <div class="mb-3">
                                            <h6>Enter course opted in vvit</h6>
                                            <select class="form-select" name="course" aria-label="course" required>
                                                <option value="<%= user.profile.course %>"><%= user.profile.course %></option>
                                                <option value="cse">CSE</option>
                                                <option value="ece">ECE</option>
                                                <option value="it">IT</option>
                                                <option value="civil">CIVIL</option>
                                                <option value="mech">MECH</option>
                                                <option value="eee">EEE</option>
                                            </select>
                                        </div>
                                    </div>
                                    <hr class="my-4">
                                    <ul class="list-group list-group-flush">
                                        <li class="list-group-item d-flex justify-content-between align-items-center flex-wrap">
                                            <h6 class="mb-0"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-globe me-2 icon-inline"><circle cx="12" cy="12" r="10"></circle><line x1="2" y1="12" x2="22" y2="12"></line><path d="M12 2a15.3 15.3 0 0 1 4 10 15.3 15.3 0 0 1-4 10 15.3 15.3 0 0 1-4-10 15.3 15.3 0 0 1 4-10z"></path></svg>Website</h6>
                                            <input type="text" class="form-control" name="websiteurl" value="<%=user.profile.website%>">
                                        </li>
                                        <li class="list-group-item d-flex justify-content-between align-items-center flex-wrap">
                                            <h6 class="mb-0"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-github me-2 icon-inline"><path d="M9 19c-5 1.5-5-2.5-7-3m14 6v-3.87a3.37 3.37 0 0 0-.94-2.61c3.14-.35 6.44-1.54 6.44-7A5.44 5.44 0 0 0 20 4.77 5.07 5.07 0 0 0 19.91 1S18.73.65 16 2.48a13.38 13.38 0 0 0-7 0C6.27.65 5.09 1 5.09 1A5.07 5.07 0 0 0 5 4.77a5.44 5.44 0 0 0-1.5 3.78c0 5.42 3.3 6.61 6.44 7A3.37 3.37 0 0 0 9 18.13V22"></path></svg>Github</h6>
                                            <input type="text" class="form-control" name="github" value="<%=user.profile.github%>">
                                        </li>
                                        <li class="list-group-item d-flex justify-content-between align-items-center flex-wrap">
                                            <h6 class="mb-0"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-twitter me-2 icon-inline text-info"><path d="M23 3a10.9 10.9 0 0 1-3.14 1.53 4.48 4.48 0 0 0-7.86 3v1A10.66 10.66 0 0 1 3 4s-4 9 5 13a11.64 11.64 0 0 1-7 2c9 5 20 0 20-11.5a4.5 4.5 0 0 0-.08-.83A7.72 7.72 0 0 0 23 3z"></path></svg>Twitter</h6>
                                            <input type="text" class="form-control" name="twitter" value="<%=user.profile.twitter%>">
                                        </li>

                                        <li class="list-group-item d-flex justify-content-between align-items-center flex-wrap">
                                            <h6 class="mb-0"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-linkedin me-2 icon-inline text-primary"><path d="M4.98 3.5c0 1.381-1.11 2.5-2.48 2.5s-2.48-1.119-2.48-2.5c0-1.38 1.11-2.5 2.48-2.5s2.48 1.12 2.48 2.5zm.02 4.5h-5v16h5v-16zm7.982 0h-4.968v16h4.969v-8.399c0-4.67 6.029-5.052 6.029 0v8.399h4.988v-10.131c0-7.88-8.922-7.593-11.018-3.714v-2.155z"/></svg>LinkedIn</h6>
                                            <input type="text" class="form-control" name="linkedin" value="<%=user.profile.linkedin%>">
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-8">

                            <h3>Professional Details</h3>
                            
                                <div class="jobs card  mb-4">
                                    <% if(user.professionaldetails) { %>
                                    <% for(var i=0;i < user.professionaldetails.length;i++) {%>
                                        
                                        <div class="card-body">
                                            <h5>Job - <%= i+1 %> </h5>
                                            <div class="row mb-3">
                                                <div class="col-sm-3">
                                                    <h6 class="mb-0">Job Title</h6>
                                                </div>
                                                <div class="col-sm-9 text-secondary">
                                                    <input type="text" class="form-control" name="jobtitle" value="<%= user.professionaldetails[i].title %>" required>
                                                </div>
                                            </div>

                                            <div class="row mb-3">
                                                <div class="col-sm-3">
                                                    <h6 class="mb-0">Type of Job</h6>
                                                </div>
                                                <div class="col-sm-9 text-secondary">
                                                    <select class="form-select" name="jobtype" aria-label="type of job" required>
                                                        <option value="<%= user.professionaldetails[i].type %>"><%= user.professionaldetails[i].type %></option>
                                                        <option value="parttime">parttime</option>
                                                        <option value="fulltime">Full Time</option>
                                                        <option value="internship">Internship</option>
                                                    </select>
                                                </div>
                                            </div>
                                        
                                            <div class="row mb-3">
                                                <div class="col-sm-3">
                                                    <h6 class="mb-0">Company</h6>
                                                </div>
                                                <div class="col-sm-9 text-secondary">
                                                    <input type="text" class="form-control" name="jobcompany" value="<%= user.professionaldetails[i].company %>" required>
                                                </div>
                                            </div>
                                            
                                            <div class="row mb-3">
                                                <div class="col-sm-3">
                                                    <h6 class="mb-0">Location</h6>
                                                </div>
                                                <div class="col-sm-9 text-secondary">
                                                    <input type="text" class="form-control" name="joblocation" value="<%= user.professionaldetails[i].location %>" required>
                                                </div>
                                            </div>
                                        

                                        
                                            <div class="row mb-3">
                                                <div class="col-md-2">
                                                    <h6 class="m-0">Start Time</h6>
                                                </div>
                                                <div class="col-md-4 text-secondary">
                                                    <select class="form-select" name="jobjoinmonth" aria-label="job month">
                                                        <option value="<%= user.professionaldetails[i].startdate.month %>"><%= user.professionaldetails[i].startdate.month %></option>
                                                        <option value="January">January</option><option value="February">February</option>
                                                        <option value="March">March</option><option value="April">April</option>
                                                        <option value="May">May</option><option value="June">June</option>
                                                        <option value="July">July</option><option value="August">August</option>
                                                        <option value="September">September</option><option value="October">October</option>
                                                        <option value="November">November</option><option value="December">December</option>
                                                    </select>
                                                    </div>
                                                    <div class="col-md-4 text-secondary">
                                                        <input type="number" min="1970" max="2100" class="form-control" name="jobjoinyear" value="<%= user.professionaldetails[i].startdate.year %>">
                                                    </div>
                                            </div>
                                            <div class="row mb-3">
                                                <div class="col-md-2">
                                                    <h6 class="m-0">End Time (Optional)</h6>
                                                </div>
                                                <div class="col-md-4 text-secondary">
                                                    <select class="form-select" name="jobleftmonth" aria-label="job month" required>
                                                        <% if(user.professionaldetails[i].enddate.month != "-1") {%>
                                                            <option value="<%= user.professionaldetails[i].enddate.month %>"><%= user.professionaldetails[i].enddate.month %></option>
                                                        <%}else{%>
                                                            <option value="-1">select a month</option>
                                                        <%}%>
                                                        <option value="January">January</option><option value="February">February</option>
                                                        <option value="March">March</option><option value="April">April</option>
                                                        <option value="May">May</option><option value="June">June</option>
                                                        <option value="July">July</option><option value="August">August</option>
                                                        <option value="September">September</option><option value="October">October</option>
                                                        <option value="November">November</option><option value="December">December</option>
                                                    </select>
                                                    </div>
                                                    <div class="col-md-4 text-secondary">
                                                    <input type="number" min="1970" max="2100" class="form-control" name="jobleftyear" value="<%= user.professionaldetails[i].enddate.year %>">
                                                </div>
                                            </div>
                                            <div class="input-group-append" style="justify-content: center;display: flex;margin: 2% 0px;"><button class="btn btn-outline-danger remove_field" type="button">Remove</button></div>
                                        </div>
                                    <%}}%>        

                                </div>
                            
                            <button  type="button" class="add_job btn btn-secondary" value="Create Element" > add another JOB details </button>

                            <h3>Educational Details</h3>
                            <div class="educational card  mb-4">
                                <% if(user.Educationaldetails) { %>
                                <% for(var i=0;i < user.Educationaldetails.length;i++) {%>
                                
                                    <div class="card-body">
                                        <div class="row mb-3">
                                            <div class="col-sm-3">
                                                <h6 class="mb-0">School</h6>
                                            </div>
                                            <div class="col-sm-9 text-secondary">
                                                <input type="text" class="form-control" name="eduschool" value="<%= user.Educationaldetails[i].school %>" required>
                                            </div>
                                        </div>

                                        <div class="row mb-3">
                                            <div class="col-sm-3">
                                                <h6 class="mb-0">Degree</h6>
                                            </div>
                                            <div class="col-sm-9 text-secondary">
                                                <select class="form-select" name="edudegree" aria-label="Degree" required>
                                                    <option value="<%= user.Educationaldetails[i].degree %>"><%= user.Educationaldetails[i].degree %></option>
                                                    <option value="Msc">Msc</option>
                                                    <option value="Mtech">Mtech</option>
                                                    <option value="phD">phD</option>
                                                </select>
                                            </div>
                                        </div>
                                    
                                        <div class="row mb-3">
                                            <div class="col-sm-3">
                                                <h6 class="mb-0">Field of Study</h6>
                                            </div>
                                            <div class="col-sm-9 text-secondary">
                                                <input type="text" class="form-control" name="edufieldofstudy" value="<%= user.Educationaldetails[i].fieldofstudy %>" required>
                                            </div>
                                        </div>
                                    
                                        <div class="row mb-3">
                                            <div class="col-md-2">
                                                <h6 class="m-0">Start Time</h6>
                                            </div>
                                            <div class="col-md-4 text-secondary">
                                                <select class="form-select" name="edujoinmonth" aria-label="educational month" required>
                                                    <option value="<%= user.Educationaldetails[i].startdate.month %>"><%= user.Educationaldetails[i].startdate.month %></option>
                                                    <option value="January">January</option><option value="February">February</option>
                                                    <option value="March">March</option><option value="April">April</option>
                                                    <option value="May">May</option><option value="June">June</option>
                                                    <option value="July">July</option><option value="August">August</option>
                                                    <option value="September">September</option><option value="October">October</option>
                                                    <option value="November">November</option><option value="December">December</option>
                                                </select>
                                            </div>
                                            <div class="col-md-4 text-secondary">
                                                <input type="number" min="1970" max="2100" class="form-control" name="edujoinyear" value="<%= user.Educationaldetails[i].startdate.year %>" required>
                                            </div>
                                        </div>

                                        <div class="row mb-3">
                                            <div class="col-md-2">
                                                <h6 class="m-0">End Time</h6>
                                            </div>
                                            <div class="col-md-4 text-secondary">
                                                <select class="form-select" name="eduendmonth" aria-label="educational month" required>
                                                    <option value="<%= user.Educationaldetails[i].enddate.month %>"><%= user.Educationaldetails[i].enddate.month %></option>
                                                    <option value="January">January</option><option value="February">February</option>
                                                    <option value="March">March</option><option value="April">April</option>
                                                    <option value="May">May</option><option value="June">June</option>
                                                    <option value="July">July</option><option value="August">August</option>
                                                    <option value="September">September</option><option value="October">October</option>
                                                    <option value="November">November</option><option value="December">December</option>
                                                </select>    
                                            </div>
                                            <div class="col-md-4 text-secondary">
                                                <input type="number" min="1970" max="2100" class="form-control" name="eduendyear" value="<%= user.Educationaldetails[i].enddate.year %>" required>
                                            </div>
                                        </div>
                                        <div class="input-group-append" style="justify-content: center;display: flex;margin: 2% 0px;"><button class="btn btn-outline-danger remove_field" type="button">Remove</button></div>
                                    </div>
                                
                                <%}}%>
                            </div>
                            <button  type="button" class="add_educationaldetail btn btn-secondary" value="Create Element" > add another Educational details </button>
                            

                            <h3>Personal Information</h3>
                            <div class="card mb-4">
                                <div class="card-body">
                                    <div class="row mb-3">
                                        <div class="col-sm-3">
                                            <h5 class="mb-0">About Me</h5>
                                        </div>
                                        <div class="col-sm-9 text-secondary">    
                                            <textarea type="textarea" class="form-control" name="aboutme" rows="3" required><%=user.personalinfo.aboutme%></textarea>
                                        </div>
                                    </div>
                                    <div class="row mb-3">
                                        <div class="col-sm-3">
                                            <h5 class="mb-0" for="dob">Date of Birth</h5>
                                        </div>
                                        <div class="col-sm-9 text-secondary">
                                            <input type="date" class="form-control" name="dob" value="<%=dob%>" required>
                                        </div>
                                    </div>

                                    

                                    
                                    
                                </div>
                            </div>
                            
                                <h3>Contact Details</h3>
                                <div class="card  mb-4">
                                    <div class="card-body">
                                        <h6> Address </h6>
                                        <div class="row mb-3">
                                            <div class="col-md-4 col-sm-12"> <input type="text" class="form-control" name="city" value="<%=user.contactdetails.address.city%>" placeholder="City"  required></div>
                                            <div class="col-md-4 col-sm-12"> <input type="text" class="form-control" name="state" value="<%=user.contactdetails.address.state%>" placeholder="State" required></div>
                                            <div class="col-md-4 col-sm-12"> <input type="text" class="form-control" name="country" value="<%=user.contactdetails.address.country%>" placeholder="Country" required></div>
                                        </div>
                                        <div class="row mb-3">
                                            <div class="col-sm-3">
                                                <h6 class="mb-0">Email</h6>
                                            </div>
                                            <div class="col-sm-9 text-secondary">
                                                <input type="text" readonly class="form-control" name="email" value="<%=user.email%>">
                                            </div>
                                        </div>
                                        <div class="row mb-3">
                                            <div class="col-sm-3">
                                                <h6 class="mb-0">Phone Number</h6>
                                            </div>
                                            <div class="col-sm-9 text-secondary">
                                                <input type="text" class="form-control" name="phone" value="<%=user.contactdetails.phone%>">
                                            </div>
                                        </div>

                                    </div>
                                </div>
                                <div class="row d-flex justify-content-end">
                                    <div class="col-sm-3 m-3"></div>
                                    <div class="col-sm-9 text-secondary">
                                        <input type="submit" class="btn btn-primary px-4" value="Save Changes">
                                    </div>
                                </div>
                            </div>
                            
                        </div>
                    </div>
                </div>
                
        </form>

        <!-- new professional details : -->
